<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="order">
	<typeAlias alias="orderVo" type="food.vo.OrderVo"/>
	<select id="selectAllOrder" resultClass="orderVo">
		SELECT * FROM FOODORDER
	</select>
	<select id="selectMemberOrder" parameterClass="java.lang.String" resultClass="orderVo">
		SELECT ORDERTIME,ORDERNUMBER,STORENAME,PRICE,STATE FROM FOODORDER WHERE MEMBERID = #memberid#
	</select>
</sqlMap>